{
  "name": "Chatbreaker",
  "version": "1.0.1",
  "author": "Ydalirsson",
  "description": "Chatbreaker is an application for separating text into single chat messages. Specified for Guild Wars 2 chat messages.",
  "license": "GNUv3.0",
  "private": true,
  "main": "background.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "electron:win": "vue-cli-service electron:build --windows portable",
    "electron:win32": "vue-cli-service electron:build --windows nsis:ia32 portable",
    "electron:win64": "vue-cli-service electron:build --windows nsis:x64 portable",
    "electron:linux": "vue-cli-service electron:build --linux",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "bootstrap": "^5.0.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "electron": "^14.0.0",
    "electron-devtools-installer": "^3.1.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue-cli-plugin-electron-builder": "*"
  }
}
